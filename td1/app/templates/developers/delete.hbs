<div class="ui container">
  <div class="ui message">
    <div class="header">
      Deletion of {{model.identity}}
    </div>
    <p>Do you want to delete this developer ?</p>
    <button class="ui button red" {{action "deleteDeveloper" model}}>
      Delete ?
    </button>
    <LinkTo @route="developers"class="btn btn-danger">Cancel</LinkTo>
  </div>
</div>
{{outlet}}